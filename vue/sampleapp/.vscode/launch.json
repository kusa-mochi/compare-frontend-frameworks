{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}",
        "breakOnLoad": true,
        "vueComponentPaths": [
          "${workspaceFolder}/**/*.vue"
        ],
        "sourceMaps": true,
        "sourceMapPathOverrides": {
          "webpack://sampleapp/*": "${webRoot}/*"
        }
      }
    ]
  }